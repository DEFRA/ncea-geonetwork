<gmd:CI_ResponsibleParty xmlns:xlink="http://www.w3.org/1999/xlink"
                         xmlns:gml="http://www.opengis.net/gml/3.2"
                         xmlns:gco="http://www.isotc211.org/2005/gco"
                         xmlns:gmd="http://www.isotc211.org/2005/gmd"
                         xmlns:gmx="http://www.isotc211.org/2005/gmx"
                         xmlns:gts="http://www.isotc211.org/2005/gts">
  <gmd:individualName>
      <gco:CharacterString>Name of the individual (optional, delete if not needed)</gco:CharacterString>
   </gmd:individualName>
  <gmd:organisationName>
      <gco:CharacterString>Name of the responsible organization</gco:CharacterString>
   </gmd:organisationName>
  <gmd:positionName>
      <gco:CharacterString>Function performed by the responsible party</gco:CharacterString>
   </gmd:positionName>
  <gmd:contactInfo>
      <gmd:CI_Contact>
         <gmd:phone>
            <gmd:CI_Telephone>
               <gmd:voice>
                  <gco:CharacterString>Telephone number</gco:CharacterString>
               </gmd:voice>
            </gmd:CI_Telephone>
         </gmd:phone>
         <gmd:address>
            <gmd:CI_Address>
               <gmd:deliveryPoint>
                  <gco:CharacterString>Address line for the location</gco:CharacterString>
               </gmd:deliveryPoint>
               <gmd:city>
                  <gco:CharacterString>Town or City</gco:CharacterString>
               </gmd:city>
               <gmd:administrativeArea>
                  <gco:CharacterString/>
               </gmd:administrativeArea>
               <gmd:postalCode>
                  <gco:CharacterString>Postal code</gco:CharacterString>
               </gmd:postalCode>
               <gmd:country>
                  <gco:CharacterString>Country</gco:CharacterString>
               </gmd:country>
               <gmd:electronicMailAddress>
                  <gco:CharacterString>Email address</gco:CharacterString>
               </gmd:electronicMailAddress>
            </gmd:CI_Address>
         </gmd:address>
      </gmd:CI_Contact>
  </gmd:contactInfo>
  <gmd:role>
      <gmd:CI_RoleCode codeList="./resources/codeList.xml#CI_RoleCode" codeListValue="pointOfContact"/>
  </gmd:role>
</gmd:CI_ResponsibleParty>